<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Continents.SocialMediaPage">
    <ContentPage Title="Facebook">
        <WebView x:Name="webViewFB" Source="https://www.facebook.com/continents.us" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
    </ContentPage>
    <ContentPage Title="Twitter">
        <WebView x:Name="webViewTW" Source="https://www.twitter.com/continents_us" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
    </ContentPage>
    <ContentPage Title="YouTube">
        <WebView x:Name="webViewYT" Source="https://www.youtube.com/channel/UCuyvOPvVXOV3TCeeJiORtgg" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
    </ContentPage>
    <ContentPage Title="LinkedIn">
        <WebView x:Name="webViewLI" Source="https://www.linkedin.com/school/the-continents-state-university" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
    </ContentPage>
</TabbedPage>
